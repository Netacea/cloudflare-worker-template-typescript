    "test": "npx nyc tape -r ts-node/register './tests/*.test.ts'",
    "lint": "npx eslint \"{tests,src}/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Netacea/cloudflare-worker-template-typescript.git"
  },
