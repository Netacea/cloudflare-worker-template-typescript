    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "url": "^0.11.0",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "wrangler": "^2.20.0"
